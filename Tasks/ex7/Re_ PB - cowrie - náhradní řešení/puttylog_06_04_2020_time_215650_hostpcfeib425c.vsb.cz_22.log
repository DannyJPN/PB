		var slideLock = false;


		function setSlideLock() {
			slideLock = true;

			var switches = slides[currentSlideIdx].querySelectorAll(".switch");
			switches = Array.prototype.slice.call(switches, 0);	   // Array.from for Internet Explorer

			switches.forEach(function (_switch) {
