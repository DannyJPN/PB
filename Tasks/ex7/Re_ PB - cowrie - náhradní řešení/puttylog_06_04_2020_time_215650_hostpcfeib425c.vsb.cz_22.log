				result = 8;

			return result;
		}


		function shiftTabsToLeft() {
			if (currentWindowIdx === (7-1))
				return;

			var lastTabStyle = window.getComputedStyle(tabs[7-1]);
			if (lastTabStyle["display"] === "inline-block")
				return;

			var firstIdx = -1;
			var visibleTabCount = getVisibleTabCount();

			for (var i = 0; i < tabsLen; i++) {
				var tabClassList = tabs[i].classList;

				if (tabClassList.contains("first")) {
					tabClassList.remove("first");

					firstIdx = i;
				} else if (tabClassList.contains("second")) {
					tabClassList.remove("second");
					tabClassList.add("first");
				} else if (tabClassList.contains("third")) {
					tabClassList.remove("third");
					tabClassList.add("second");
				} else if (tabClassList.contains("fourth")) {
					tabClassList.remove("fourth");
