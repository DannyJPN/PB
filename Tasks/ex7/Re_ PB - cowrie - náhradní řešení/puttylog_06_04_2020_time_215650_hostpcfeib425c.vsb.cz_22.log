		function showPrevWindow() {
			if (currentWindowIdx > 0)
				showWindowWithIdx(currentWindowIdx - 1, true);
		}
		window.tabwindowsomkx2.showPrevWindow = showPrevWindow;


		function showNextWindow() {
			if (currentWindowIdx < (7 - 1))
				showWindowWithIdx(currentWindowIdx + 1, true);
		}
		window.tabwindowsomkx2.showNextWindow = showNextWindow;


		function showWindowWithIdx(idx, _shiftTabs) {
			var shiftTabs = _shiftTabs || false;

			if (shiftTabs) {
				if (idx > currentWindowIdx)
					shiftTabsToLeft();
				else
					shiftTabsToRight();
			}

			tabs[currentWindowIdx].classList.remove("active");
			windows[currentWindowIdx].classList.add("old-active");
			windows[currentWindowIdx].classList.remove("active");

			var oldWindowIdx = currentWindowIdx;
			currentWindowIdx = idx;

			windows[currentWindowIdx].classList.add("active");
			tabs[currentWindowIdx].classList.add("active");

			handleNavButtons();
			handleSeparators(tabs[currentWindowIdx], "click");
			handleLeftLinkSeparators(windows[currentWindowIdx]);

			// reload image - navigation arrows can have 2 rows
			setTimeout(loadBackgroundImage, 100);

