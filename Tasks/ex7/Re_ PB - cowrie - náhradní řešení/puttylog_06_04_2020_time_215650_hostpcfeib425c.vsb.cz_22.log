					tabClassList.remove("sixth");
					tabClassList.add("seventh");
				} else if (tabClassList.contains("fifth")) {
					tabClassList.remove("fifth");
					tabClassList.add("sixth");
				} else if (tabClassList.contains("fourth")) {
					tabClassList.remove("fourth");
					tabClassList.add("fifth");
				} else if (tabClassList.contains("third")) {
					tabClassList.remove("third");
					tabClassList.add("fourth");
				} else if (tabClassList.contains("second")) {
					tabClassList.remove("second");
					tabClassList.add("third");
				} else if (tabClassList.contains("first")) {
					tabClassList.remove("first");
					tabClassList.add("second");

					secondIdx = i;
				} else if (i === ((secondIdx - 1))) {
					tabClassList.add("first");
				}
			}
		}


		function handleVisibleTabs() {
			var visibleTabCount = getVisibleTabCount();

			var firstVisibleTabIdx = -1;
			if ((7 - currentWindowIdx) < visibleTabCount)
				firstVisibleTabIdx = 7 - visibleTabCount;
			else
				firstVisibleTabIdx = currentWindowIdx;

			for (var i = 0; i < 7; i++) {
				var classList = tabs[i].classList;

				while (classList.length > 0) {
				   classList.remove(classList.item(0));
				}

				classList.add("tab");

				if (i === currentWindowIdx)
					classList.add("active");

				if ((i >= firstVisibleTabIdx) && (i <= (firstVisibleTabIdx + visibleTabCount - 1)))
					classList.add(tabOrderClasses[i - firstVisibleTabIdx]);
			}
		}


