			if ((currentSlideIdx - 1) === -1) {
				currentSlideIdx = slides.length - 1;
				nextSlideIdx = 0;
			} else {
				nextSlideIdx = currentSlideIdx--;
			}

			setSlideLock();

			if (fromButton) {
				clearInterval(timer);
				timer = null;
			}

			if (window.matchMedia("(min-width: 1201px)").matches) {
				slides[currentSlideIdx].style["z-index"] = "1";
