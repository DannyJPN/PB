					tabClassList.remove("eighth");
					tabClassList.add("seventh");
				} else if ((firstIdx > -1) && (i === (visibleTabCount + firstIdx))) {	// handle new last visible tab
					tabClassList.add(tabOrderClasses[visibleTabCount-1]);
					break;
				}
			}
		}


		function shiftTabsToRight() {
			if (currentWindowIdx === 0)
				return;

			var firstVisibleTab = document.querySelector("#tabwindowsomkx2 .tab.first");
