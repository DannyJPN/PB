			var firstIdx = -1;
			var visibleTabCount = getVisibleTabCount();

			for (var i = 0; i < tabsLen; i++) {
				var tabClassList = tabs[i].classList;

				if (tabClassList.contains("first")) {
					tabClassList.remove("first");

					firstIdx = i;
