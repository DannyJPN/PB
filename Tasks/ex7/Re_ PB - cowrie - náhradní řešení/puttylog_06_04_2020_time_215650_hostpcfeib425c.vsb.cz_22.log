		// ---- dynamic separator handling -----
		function handleSeparators(tab, action) {
			var prevTab = tab.previousSibling;

			if (prevTab === null)
				return;

			if (prevTab.classList === undefined)
				return;

			switch (action) {
				case "focus":
					prevTab.classList.add("hide-sep");
				break;
				case "blur":
					if (!tab.classList.contains("active"))
						prevTab.classList.remove("hide-sep");
				break;
				case "click":
					prevTab.classList.add("hide-sep");

					tabs.forEach(function (tab) {
						if (tab !== prevTab)
							tab.classList.remove("hide-sep");
					});
				break;
			}
		}
		window.tabwindowsomkx2.handleSeparators = handleSeparators;


		// ---- dynamic left link separators handling -----
		function handleLeftLinkSeparators(currentWindow) {
			var links = currentWindow.querySelectorAll(".left-link");
			links = Array.prototype.slice.call(links, 0);	   // Array.from for Internet Explorer
			var linksLen = links.length;

			if (linksLen > 1) {
				var prevLinkTop = links[linksLen-1].getBoundingClientRect().top;

				var insertSpace = false;

				for (var i = linksLen - 2; i > -1; i--) {
					var link = links[i];
					var linkTop = link.getBoundingClientRect().top;

					if (window.innerWidth <= 450) {
						link.classList.add("hide-sep");

						link.classList.remove("space-left");
						link.classList.add("space-both");

					} else if ((i === 0) && (insertSpace)) {
						link.classList.add("hide-sep");

						link.classList.remove("space-both");
						link.classList.add("space-left");

					} else if (linkTop < prevLinkTop) {
						link.classList.add("hide-sep");
						link.classList.remove("space-both");

						if ((insertSpace) && ((i + 1) <= (linksLen - 1))) {
							links[i+1].classList.add("space-left");
							insertSpace = false;
						}
					} else {
						link.classList.remove("hide-sep");
						link.classList.remove("space-both");
						link.classList.remove("space-left");
					}

					prevLinkTop = linkTop;
				}
			}
		}


		// ----- event handlers -----
		function redraw() {
			switchHeadlinePos();

			equalizeTabHeights();
			//equalizeWindowHeights();
			setSingleWindowHeight();

			handleVisibleTabs();
			handleNavButtons();
			handleSeparators(tabs[currentWindowIdx], "click");
			handleLeftLinkSeparators(windows[currentWindowIdx]);
