		window.tabwindowsomkx2.showWindowWithIdx = showWindowWithIdx;


		// ---- headline pos switching ----
		function switchHeadlinePos() {
			windows.forEach(function (_window) {
				var imagePart = _window.querySelector(".image-part");
				var textPart = _window.querySelector(".text-part");

				if (imagePart !== null) {
					var headline = _window.querySelector(".headline");
					var headlineOnTop = (_window.querySelector(".image-part > .headline") !== null);

					if (headlineOnTop && window.matchMedia("(min-width: 981px)").matches) {
						imagePart.removeChild(headline);
						textPart.insertBefore(headline, _window.querySelector(".text"));
					} else if (!headlineOnTop && !window.matchMedia("(min-width: 981px)").matches) {
						textPart.removeChild(headline);
						imagePart.insertBefore(headline, _window.querySelector(".image-part > .inner-image"));
					}
				}
			});
		}


		// ---- height equalizing ----
		function equalizeTabHeights() {
			var maxHeight = 0;
			tabs.forEach( function (tab) {
				var height = tab.getBoundingClientRect().height;
				if (height > maxHeight)
					maxHeight = height;
			});

			tabs.forEach( function (tab) {
				tab.style.height = maxHeight + "px";

				var tabText = tab.querySelector("span");

				var tabStyle = window.getComputedStyle(tab);
				var padding = parseFloat(tabStyle["padding-left"].replace("px", "")) + parseFloat(tabStyle["padding-right"].replace("px", ""));

				tabText.style.width = "calc(100% - " + padding + "px)";
				tabText.style.position = "absolute";
				tabText.style.top = "50%";
				tabText.style.left = "50%";
				tabText.style["-webkit-transform"] = "translateX(-50%) translateY(-50%)";
				tabText.style.transform = "translateX(-50%) translateY(-50%)";
			});
		}


		function setWrapperHeight(mainPartHeight) {
			var wrapper = document.querySelector("#tabwindowsomkx2 .windows");
			var wrapperStyle = window.getComputedStyle(wrapper);

			var linksPart = wrapper.querySelector(".links-part");

			if (linksPart === null) {
				linksPartHeight = 0;
			} else {
				var linksPartStyle = window.getComputedStyle(linksPart);
				var linksPartHeight = linksPart.getBoundingClientRect().height;
				linksPartHeight += parseFloat(linksPartStyle["margin-top"].replace("px", ""))
					+ parseFloat(linksPartStyle["margin-bottom"].replace("px", ""));
			}

			var topPadding = parseFloat(wrapperStyle["padding-top"].replace("px", ""));
			var bottomPadding = parseFloat(wrapperStyle["padding-bottom"].replace("px", ""));

			var wrapperHeight = topPadding + mainPartHeight + linksPartHeight + bottomPadding;
			wrapper.style.height = wrapperHeight + "px";
		}


		function getImagePartHeight() {
