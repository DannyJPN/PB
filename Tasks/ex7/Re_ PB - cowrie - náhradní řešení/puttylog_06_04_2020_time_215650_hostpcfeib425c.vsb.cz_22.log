		}


		function unsetSlideLock() {
			var switches = slides[currentSlideIdx].querySelectorAll(".switch");
			switches = Array.prototype.slice.call(switches, 0);	   // Array.from for Internet Explorer
			switches.forEach(function (_switch) {
				_switch.style.cursor = "pointer";
			});

			slideLock = false;
		}


		function hideOldSlide(oldSlideIdx, newSlideIdx) {
			slides[oldSlideIdx].style.display = "none";
			if (slides[oldSlideIdx].classList.contains("initial-slide"))
				slides[oldSlideIdx].classList.remove("initial-slide");

			slides[newSlideIdx].style["z-index"] = "0";
			unsetSlideLock();
		}


		function setCarouselArrowsPosition() {
			var imageContainer = slides[currentSlideIdx].querySelector(".image-container");

			var arrows = document.querySelectorAll("#slideshowXfXnT .carousel-arrow");
			arrows = Array.prototype.slice.call(arrows, 0);	   // Array.from for Internet Explorer

			arrows.forEach(function (arrow) {
				arrow.style.top = ((imageContainer.getBoundingClientRect().height - arrow.getBoundingClientRect().height) / 2) + "px";
			});
		}


		function showSlideIdx(idx) {
			if (currentSlideIdx === idx)
				return;

			if (slideLock)
				return;

			var _slideIdx = currentSlideIdx;
			currentSlideIdx = idx;

			setSlideLock();

			clearInterval(timer);
			timer = null;

			if (window.matchMedia("(min-width: 1201px)").matches) {
				slides[currentSlideIdx].style["z-index"] = "1";
				setTimeout(function () { hideOldSlide(_slideIdx, currentSlideIdx); }, 600);
			} else {
				hideOldSlide(_slideIdx, currentSlideIdx);
			}

			slides[currentSlideIdx].style.display = "block";
			limitTextField(true);
