				window.dispatchEvent(event);
			}

			if (fromButton)
				startSliding();
		}
		window.slideshowXfXnT.showPrevSlide = showPrevSlide;


		function showNextSlide(_fromButton) {
			var fromButton = _fromButton || false;

			if (slideLock)
				return;

			if ((currentSlideIdx + 1) === slides.length) {
				currentSlideIdx = 0;
				prevSlideIdx = slides.length - 1;
			} else {
				prevSlideIdx = currentSlideIdx++;
			}

			setSlideLock();

			if (fromButton) {
				clearInterval(timer);
				timer = null;
			}

			if (window.matchMedia("(min-width: 1201px)").matches) {
