		window.slideshowXfXnT.showNextSlide = showNextSlide;


		var timer = null;
		function startSliding() {
			if (timer !== null)
				clearInterval(timer);

			timer = setInterval(showNextSlide, 5 * 1000);
		}


		/* -------- text field content limitation -------- */

		function getTextFieldId(slideIdx) {
			return "vsb-slideshowXfXnT-" + slideIdx;
		}


		function saveTextFieldsContent() {
			var slidesLen = slides.length;

			for (var i = 0; i < slidesLen; i++) {
				var limiter = slides[i].querySelector(".limiter");
				if (limiter === null)
					continue;

				var content = limiter.innerHTML;
				sessionStorage.setItem(getTextFieldId(i), content);
			}
		}


		function getCurrentLimiter() {
			if (window.matchMedia("(min-width: 1201px)").matches)
				return slides[currentSlideIdx].querySelector(".text-container.default .limiter")
			else
				return slides[currentSlideIdx].querySelector(".text-container.right .limiter")
		}


		function loadTextFieldContent() {
			var limiter = getCurrentLimiter();

			if (limiter !== null) {
				var content = sessionStorage.getItem(getTextFieldId(currentSlideIdx));
				limiter.innerHTML = content;
			}
		}


		function setLimiterHeight() {
			var limiter = getCurrentLimiter();
			if (limiter === null)
				return;

			if (window.matchMedia("(max-width: 1200px)").matches) {
				//limiter.style["max-height"] = "150px";
				return;
			}

			var textField = slides[currentSlideIdx].querySelector(".text-container");

			var readMoreButton = slides[currentSlideIdx].querySelector(".button-container");
			var switchContainer = slides[currentSlideIdx].querySelector(".switch-container");

			var computedStyle = getComputedStyle(textField);
			var maxHeight = textField.offsetHeight - parseFloat(computedStyle["padding-top"].replace("px", "").trim())
				- parseFloat(computedStyle["padding-bottom"].replace("px", "").trim());

			if (readMoreButton !== null)
				maxHeight -= readMoreButton.offsetHeight;

			if (switchContainer !== null)
				maxHeight -= switchContainer.offsetHeight;

			limiter.style["max-height"] = maxHeight + "px";
		}


		function setLimiterContent() {
			var limiter = getCurrentLimiter();
			if (limiter === null)
				return;

			var contentNodes = limiter.childNodes;
			var contentNodesLen = contentNodes.length;

			var cnt = 0;
			var limiterMaxHeight = parseInt(limiter.style["max-height"]);
			for (var i = contentNodesLen-1; i > -1; i--) {
				if ((limiter.scrollHeight - limiter.offsetHeight) < 2) {
					if (cnt === 0)
						break;

					if ((cnt > 0) && (!contentNodes[i].innerText))
						continue;

					//contentNodes[i].innerText += "...";
					break;
				}

				if (contentNodes[i].innerText) {
					var contentArray = contentNodes[i].innerText.split(" ");

					while (contentArray.length > 0) {
						contentArray.pop();
						contentNodes[i].innerText = contentArray.join(" ").trim();

						if (limiter.offsetHeight < limiterMaxHeight) {
							break;
						}

						cnt++;
					}

					if ((contentArray.length > 0) && (cnt > 0)) {
						if ( contentNodes[i].nodeName == "UL" ) {
							var listItems = contentNodes[i].innerText.split("\n");
							var output = "";
							for (var x = 0; x < listItems.length; x++) {
								output += "<li>";
								output += listItems[x];
								if (x + 1 == listItems.length) {
									output += "...";
								}
								output += "</li>";
							}
							contentNodes[i].innerHTML = output;
						} else {
							contentNodes[i].innerText = contentArray.join(" ").trim() + "...";
						}

						break;
					} else if (contentArray.length > 0) {
						contentNodes[i].innerText = contentArray.join(" ").trim();
						break;
					} else
						contentNodes[i].innerText = "";
				}
			}
			var switchContainer = slides[currentSlideIdx].querySelector(".switch-container");
			if (switchContainer !== null) {
				switchContainer.style["visibility"] = "visible";
			}
		}


		function limitTextField(_loadContent) {
			var loadContent = _loadContent || false;

			var limiter = getCurrentLimiter();
			if (limiter === null)
